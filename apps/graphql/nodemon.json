{
	"watch": ["src/**", "config/**"],
	"exec": "ts-node -r dotenv/config -r tsconfig-paths/register ./apps/graphql/src/main.ts dotenv_config_path=./apps/graphql/config/development/.env",
	"restartable": "rs",
	"verbose": false,
	"ignore": ["src/**/*.spec.ts", "/node_modules/**"],
	"ext": "js,ts, jade, tsx, jsx, mjs"
}
