{
  "name": "nestjs-monorepo",
  "version": "0.0.1",
  "description": "Monorepo containing apps - graphql redis abstract database",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild:graphql": "rimraf dist/apps/graphql",
    "prebuild:es": "rimraf dist/apps/es",
    "build": "export NODE_ENV=dev && nest build",
    "build:scaffold:graphql:dev" : "nest build graphql",
    "build:scaffold:graphql:prod" : "nest build graphql",
    "build:graphql:dev": "export NODE_ENV=dev && npm run prebuild:graphql && rimraf target && chmod -R 755 ./apps/graphql/src/build/ && ./apps/graphql/src/build/build.sh",
    "build:graphql:dev:circleci": "export NODE_ENV=dev && npm run prebuild:graphql && rimraf target && chmod -R 755 ./apps/graphql/src/build/ && bash ./apps/graphql/src/build/build.sh",
    "build:babel:graphql": "npm run prebuild:graphql && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:webpack:graphql:prod": "NODE_ENV=prod && npm run prebuild:graphql && webpack --config apps/graphql/src/deployment/webpack-manifest/webpack.config.js",
    "build:webpack:graphql:dev": "NODE_ENV=dev && npm run prebuild:graphql && webpack --config apps/graphql/src/deployment/webpack-manifest/webpack.dev.config.js",
    "build:webpack:dev:es": "NODE_ENV=dev && npm run prebuild:es && webpack --config ./apps/es/webpack.dev.config.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "offline:deploy": "npm run build && export NODE_ENV=dev && SLS_DEBUG=* sls offline --stage dev -v",
    "aws:dev:deploy": "npm run build && export NODE_ENV=dev && SLS_DEBUG=* sls deploy --stage dev -v",
    "start": "nest start",
    "start:nodemon:dev:graphql": "NODE_ENV=dev && nodemon --config apps/graphql/nodemon.json",
    "start:nodemon:dev:es": "NODE_ENV=dev && nodemon --config apps/es/nodemon.json",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config apps/graphql/test/jest-e2e.json"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.15.0",
    "@graphql-tools/schema": "^8.3.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.1.1",
    "@nestjs/platform-fastify": "^8.0.7",
    "apollo-server-fastify": "^3.4.0",
    "atob": "^2.1.2",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.918.0",
    "btoa": "^1.2.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "graphql": "^15.6.1",
    "gridfs-stream": "^1.1.1",
    "kafkajs": "^1.15.0",
    "mongoose": "^5.13.12",
    "neo4j-driver": "^4.3.4",
    "performance-now": "^2.1.0",
    "pump": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "subscriptions-transport-ws": "^0.9.19",
    "undici": "^4.7.0",
    "uuid": "^8.3.2",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@hewmen/serverless-plugin-typescript": "^1.1.17",
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/aws-lambda": "^8.10.76",
    "@types/aws-sdk": "^2.7.0",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/gridfs-stream": "^0.5.35",
    "@types/jest": "^27.0.1",
    "@types/kafkajs": "^1.9.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@types/uuid-validate": "0.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "pino-pretty": "^5.0.1",
    "prettier": "^2.3.2",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-typescript": "0.0.5",
    "supertest": "^6.1.3",
    "terser-webpack-plugin": "^5.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "typescript-tslint-plugin": "^1.0.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1"
    }
  }
}
